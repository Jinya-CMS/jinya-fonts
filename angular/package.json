{
  "name": "jinya-fonts",
  "version": "3.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:admin && npm run build:frontend",
    "build:admin": "ng build admin",
    "build:frontend": "ng build frontend",
    "build:dev": "npm run build:frontend:dev && npm run build:admin:dev",
    "build:admin:dev": "ng build --configuration development admin",
    "build:frontend:dev": "ng build --configuration development frontend",
    "watch:admin": "ng build --watch --configuration development admin",
    "watch:frontend": "ng build --watch --configuration development frontend",
    "test": "ng test",
    "lint": "prettier --check projects && ng lint",
    "format": "prettier --write projects"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "19.0.1",
    "@angular/common": "19.0.1",
    "@angular/compiler": "19.0.1",
    "@angular/core": "19.0.1",
    "@angular/forms": "19.0.1",
    "@angular/platform-browser": "19.0.1",
    "@angular/platform-browser-dynamic": "19.0.1",
    "@angular/router": "19.0.1",
    "@fontsource/albert-sans": "5.1.0",
    "@fontsource/oxygen-mono": "5.1.0",
    "@fontsource/urbanist": "5.1.0",
    "@jinyacms/cosmo": "1.0.4",
    "@tinymce/tinymce-angular": "8.0.1",
    "angular-oauth2-oidc": "17.0.2",
    "lucide-angular": "0.462.0",
    "rxjs": "7.8.1",
    "tinymce": "7.5.1",
    "tslib": "2.8.1",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.0.2",
    "@angular-eslint/builder": "18.4.2",
    "@angular/cli": "19.0.2",
    "@angular/compiler-cli": "19.0.1",
    "@angular/localize": "19.0.1",
    "@eslint/js": "9.15.0",
    "angular-eslint": "18.4.2",
    "eslint": "9.15.0",
    "prettier": "3.4.1",
    "typescript": "5.6.3",
    "typescript-eslint": "8.16.0"
  }
}
