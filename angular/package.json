{
  "name": "jinya-fonts",
  "version": "3.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:admin && npm run build:frontend",
    "build:admin": "ng build admin",
    "build:frontend": "ng build frontend",
    "build:dev": "npm run build:frontend:dev && npm run build:admin:dev",
    "build:admin:dev": "ng build --configuration development admin",
    "build:frontend:dev": "ng build --configuration development frontend",
    "watch:admin": "ng build --watch --configuration development admin",
    "watch:frontend": "ng build --watch --configuration development frontend",
    "test": "ng test",
    "lint": "prettier --check projects && ng lint",
    "format": "prettier --write projects"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "18.2.8",
    "@angular/common": "18.2.8",
    "@angular/compiler": "18.2.8",
    "@angular/core": "18.2.8",
    "@angular/forms": "18.2.8",
    "@angular/platform-browser": "18.2.8",
    "@angular/platform-browser-dynamic": "18.2.8",
    "@angular/router": "18.2.8",
    "@fontsource/albert-sans": "5.1.0",
    "@fontsource/oxygen-mono": "5.1.0",
    "@fontsource/urbanist": "5.1.0",
    "@jinyacms/cosmo": "1.0.3",
    "@tinymce/tinymce-angular": "8.0.1",
    "angular-oauth2-oidc": "17.0.2",
    "lucide-angular": "0.453.0",
    "rxjs": "7.8.1",
    "tinymce": "7.4.1",
    "tslib": "2.8.0",
    "zone.js": "0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.2.9",
    "@angular-eslint/builder": "18.4.0",
    "@angular/cli": "18.2.9",
    "@angular/compiler-cli": "18.2.8",
    "@angular/localize": "18.2.8",
    "@eslint/js": "9.13.0",
    "angular-eslint": "18.4.0",
    "eslint": "9.13.0",
    "prettier": "3.3.3",
    "typescript": "5.4.5",
    "typescript-eslint": "8.11.0"
  }
}
